{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
      "@services/*": ["src/services/*"],
      "@config/*":["src/config/*"]
    },
    "typeRoots": ["./src/config/types"]
  }
}
