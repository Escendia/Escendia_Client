{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@config/*": [
        "src/config/*"
      ]
    },
    "typeRoots": [
      "./src/config/types"
    ]
  },
  "extends": "expo/tsconfig.base"
}
